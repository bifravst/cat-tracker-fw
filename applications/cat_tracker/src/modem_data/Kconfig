menu "MODEM_DATA"

config MODEM_DATA
	bool "Interface for modem info module"
	default y
	select MODEM_INFO

if MODEM_DATA

module = MODEM_DATA
module-str = modem info interface
source "${ZEPHYR_BASE}/subsys/logging/Kconfig.template.log_config"

endif # MODEM_DATA

endmenu